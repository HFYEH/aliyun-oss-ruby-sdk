<h2 id="post-form">POST Object</h2>

<hr/>

<%= form_tag @bucket_endpoint, method: :post, multipart: true, class: 'form-horizontal' do -%>
  <%= hidden_field_tag 'OSSAccessKeyId', @access_key %>
  <%= hidden_field_tag 'policy', @policy %>
  <%= hidden_field_tag 'Signature', @signature %>
  <%= hidden_field_tag 'key', @key %>
  <%= hidden_field_tag 'success_action_redirect', @return_url %>
  <%= hidden_field_tag 'x-oss-meta-user', @username %>
  <%= hidden_field_tag 'x-oss-object-acl', @acl %>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label required">File</label>
    <%= file_field_tag 'file' %>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Upload TO OSS</button>
    </div>
  </div>
<% end -%>

<h2 id="put-form">PUT Object</h2>

<hr/>

<%= form_tag '/new_put', method: :post, multipart: true, class: 'form-horizontal' do -%>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label required">Name</label>
    <div class="col-sm-10">
      <%= text_field_tag 'name', nil, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label required">File</label>
    <%= file_field_tag 'file' %>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Upload</button>
    </div>
  </div>
<% end -%>
